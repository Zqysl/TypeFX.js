{
  "name": "typefxjs",
  "version": "1.0.1",
  "description": "A modern typewriter effect library in plain JavaScript",
  "license": "MIT",
  "author": "Qingyu Zhang",
  "types": "dist/typefx.d.ts",
  "exports": {
    ".": {
      "types": "./dist/typefx.d.ts",
      "import": "./dist/typefx.js",
      "require": "./dist/typefx.cjs"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "test": "node test/server.mjs",
    "prepublishOnly": "pnpm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "type": "module",
  "keywords": [
    "type",
    "typed",
    "typing",
    "animation",
    "typewriter",
    "text"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup": "^4.49.0",
    "typescript": "^5.9.2",
    "tslib": "^2.6.2",
    "express": "^5.2.1"
  }
}
