{
  "name": "typefxjs",
  "version": "0.0.8",
  "description": "A modern typewriter effect library in plain JavaScript",
  "license": "MIT",
  "author": "Qingyu Zhang",
  "types": "dist/typefx.d.ts",
  "exports": {
    ".": {
      "types": "./dist/typefx.d.ts",
      "import": "./dist/typefx.js",
      "require": "./dist/typefx.cjs"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && rollup -c",
    "test": "node test/server.js",
    "prepublishOnly": "pnpm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "type",
    "typed",
    "typing",
    "animation",
    "typewriter",
    "text"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "express": "^5.2.1",
    "rollup": "^4.49.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
